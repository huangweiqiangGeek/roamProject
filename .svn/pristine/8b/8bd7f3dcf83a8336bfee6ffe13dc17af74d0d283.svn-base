package com.guoll.modules.sysmanage.bean;

import com.guoll.modules.framework.base.BaseModel;

public class SysPost extends BaseModel{

	private Integer id;//主键
	private String name;//岗位名称
	private String job;//机构id
	private String remark;//备注
	private Integer org_id;
	private Integer user_id;
	private Integer role_id;
	private String entity_ids;
	private String provinceSpell;
	private SysProvince sysProvince;
	
	public String getProvinceSpell() {
		return provinceSpell;
	}
	public void setProvinceSpell(String provinceSpell) {
		this.provinceSpell = provinceSpell;
	}
	public SysProvince getSysProvince() {
		return sysProvince;
	}
	public void setSysProvince(SysProvince sysProvince) {
		this.sysProvince = sysProvince;
	}
	public Integer getId() {
		return id;
	}
	public void setId(Integer id) {
		this.id = id;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getJob() {
		return job;
	}
	public void setJob(String job) {
		this.job = job;
	}
	public String getRemark() {
		return remark;
	}
	public void setRemark(String remark) {
		this.remark = remark;
	}
	public Integer getOrg_id() {
		return org_id;
	}
	public void setOrg_id(Integer org_id) {
		this.org_id = org_id;
	}
	public Integer getUser_id() {
		return user_id;
	}
	public void setUser_id(Integer user_id) {
		this.user_id = user_id;
	}
	
	public Integer getRole_id() {
		return role_id;
	}
	public void setRole_id(Integer role_id) {
		this.role_id = role_id;
	}
	public String getEntity_ids() {
		return entity_ids;
	}
	public void setEntity_ids(String entity_ids) {
		this.entity_ids = entity_ids;
	}
}