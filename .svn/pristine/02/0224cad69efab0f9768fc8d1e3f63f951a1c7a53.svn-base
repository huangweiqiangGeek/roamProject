function removeMethod(id){
	$.messager.confirm('确认对话框', '您确定删除此条产品组信息吗？', function(r){
		if (r){
			$.ajax({
				type:"get",
				url:"productGroupController/deleteProductGroupById?id="+id,
				async:true,
				error:function(){
					$.messager.alert('溫馨提示','删除失败!');
				},
				success:function(data){
					if(data =="0"){
						$.messager.alert('溫馨提示','删除成功!');
					}else{
						$.messager.alert('溫馨提示','删除失败!');
					}
					$('#dg').datagrid('reload');
				}
			})
		}
	});
	
}

