package com.guoll.modules.commType.bean;

import com.guoll.modules.framework.base.BaseModel;

/**
 * 通用类型表
 * 
 * @author lukas 414024003@qq.com
 * @version 1.0
 *
 */
public class CommType  extends BaseModel{
	private Integer id;// 主键自增标识
	private String commTypeName;// 类型名称
	/**
	 * 1.话单模版状态、2.预设结果模版状态、3.项目状态、4.用例状态、5.执行结果状态
	 * 6.选择方式状态 7.用例执行状态
	 */
	private Integer affiliationType;// 固定模块

	public Integer getId() {
		return id;
	}

	public void setId(Integer id) {
		this.id = id;
	}

	public String getCommTypeName() {
		return commTypeName;
	}

	public void setCommTypeName(String commTypeName) {
		this.commTypeName = commTypeName;
	}

	public Integer getAffiliationType() {
		return affiliationType;
	}

	public void setAffiliationType(Integer affiliationType) {
		this.affiliationType = affiliationType;
	}

	@Override
	public String toString() {
		return "CommType [id=" + id + ", commTypeName=" + commTypeName + ", affiliationType=" + affiliationType + "]";
	}
	
}
