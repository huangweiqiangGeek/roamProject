package com.guoll.modules.homeproject.bean;

import com.guoll.modules.framework.base.BaseModel;

/**
 * 项目类
 * @version 170818
 * 工具第二版新加模块
 * 修改需求：原有的项目改为任务，任务来管理下面的用例，新增一个项目，项目管理任务，
 * 所有的项目统一归为资费测试项目（左侧菜单）管理
 * @author htnm
 * 
 */
public class HomeProject extends BaseModel{
	private Integer id;
	private String name;
	private String provinceSpell;//所属省份
	private String introduction;//描述信息
	private String postName;//测试岗位名称
	public Integer getId() {
		return id;
	}
	public void setId(Integer id) {
		this.id = id;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getProvinceSpell() {
		return provinceSpell;
	}
	public void setProvinceSpell(String provinceSpell) {
		this.provinceSpell = provinceSpell;
	}
	public String getIntroduction() {
		return introduction;
	}
	public void setIntroduction(String introduction) {
		this.introduction = introduction;
	}
	public String getPostName() {
		return postName;
	}
	public void setPostName(String postName) {
		this.postName = postName;
	}
	@Override
	public String toString() {
		return "HomeProject [id=" + id + ", name=" + name + ", provinceSpell=" + provinceSpell + ", introduction="
				+ introduction + ", postName=" + postName + "]";
	}
}
