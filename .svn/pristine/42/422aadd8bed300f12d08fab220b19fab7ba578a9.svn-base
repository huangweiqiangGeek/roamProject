function saveconfiguration(){
	var ipAddress=$('#configuration input[name="ipAddress"]').val();
	if(isNull(ipAddress)){
		$.messager.alert('温馨提示','请输入服务器地址');
		return false;
	}
	var hostName=$('#configuration input[name="hostName"]').val();
	if(isNull(hostName)){
		$.messager.alert('温馨提示','请输入用户名');
		return false;
	}
	var hostPassWord=$('#configuration input[name="hostPassWord"]').val();
	if(isNull(hostPassWord)){
		$.messager.alert('温馨提示','请输入用户密码');
		return false;
	}
	var field1=$('#configuration input[name="field1"]').val();
	if(isNull(field1)){
		$.messager.alert('温馨提示','请输入脚本路径');
		return false;
	}
	var field2=$('#configuration input[name="field2"]').val();
	if(isNull(field2)){
		$.messager.alert('温馨提示','请输入脚本名称');
		return false;
	}
	var field3=$('#configuration input[name="field3"]').val();
	var field4=$('#configuration input[name="field4"]').val();
	var field5=$('#configuration input[name="field5"]').val();
	var data='{"ipAddress":"'+ipAddress+'","hostName":"'+hostName+'","hostPassWord":"'+hostPassWord+'","field1":"'+field1;
		data+='","field2":"'+field2+'","field3":"'+field3+'","field4":"'+field4;
		data+='","field5":"'+field5+'"}';
	saveMethod(data);	
}

