function saveMethod(val){
	$.ajax({
		url:"commConfig/saveSysConfig",
		type:"POST",	
		contentType: "application/json",
		dataType: "text",
		data:val,	
		success:function(data){
			window.parent.$("#dg").datagrid("reload");	
			if(data==0){
				$.messager.confirm('', '数据保存失败', function(){
				});
			}else if(data==1){
				$.messager.confirm('', '数据保存成功', function(){
				});
			}
		}
	})
}
